<div class="panel">
	<div class="row panel-body">
		<% @photos.reverse.each do |photo| %>
		<div class="col-md-4" style="border: 1px solid #ffffff">
			<img src="<%= photo.banana_photo_url(:thumb) %>">
			<p><%= photo.user.username %></p>
			<p><%= photo.message %></p>
		</div>
		<% end %>
	</div>
</div>
